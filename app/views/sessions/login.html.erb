

  <h1> Sign Back In </h1>

  <div class = "form">
  
  <%= form_for (@seller), url: "/login" do |f| %>
  
    <%= f.label :email, "What is your email?"%></br>
    <%= f.email_field :email %> </br><br>
  
    <%= f.label :password %> </br>
    <%= f.password_field :password %> </br><br>
  
  
    <%= f.submit "Log In" %>
  <% end %>
</div>